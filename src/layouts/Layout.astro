---

import Header from '../components/Header.astro'
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/samplerlogo.jpeg" />
		<meta name="generator" content={Astro.generator} />

		<title>{title}</title>
	</head>
	<body>
		
		<slot />
		

	</body>
</html>
<style is:global>

@font-face {
    font-family: 'MDGROTESQUE';
    src: url('MDGROTESQUE-Bold.eot');
    src: local('MDGROTESQUE Bold'), local('MDGROTESQUE-Bold'),
        url('./font/MDGROTESQUE-Bold.eot?#iefix') format('embedded-opentype'),
        url('./font/MDGROTESQUE-Bold.woff2') format('woff2'),
        url('./font/MDGROTESQUE-Bold.woff') format('woff'),
        url('./font/MDGROTESQUE-Bold.ttf') format('truetype');
    font-weight: bold;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'MDGROTESQUE';
    src: url('MDGROTESQUE-ThinItalic.eot');
    src: local('MDGROTESQUE Thin Italic'), local('MDGROTESQUE-ThinItalic'),
        url('./font/MDGROTESQUE-ThinItalic.eot?#iefix') format('embedded-opentype'),
        url('./font/MDGROTESQUE-ThinItalic.woff2') format('woff2'),
        url('./font/MDGROTESQUE-ThinItalic.woff') format('woff'),
        url('./font/MDGROTESQUE-ThinItalic.ttf') format('truetype');
    font-weight: 100;
    font-style: italic;
    font-display: swap;
}

@font-face {
    font-family: 'MDGROTESQUE';
    src: url('MDGROTESQUE-Thin.eot');
    src: local('MDGROTESQUE Thin'), local('MDGROTESQUE-Thin'),
        url('./font/MDGROTESQUE-Thin.eot?#iefix') format('embedded-opentype'),
        url('./font/MDGROTESQUE-Thin.woff2') format('woff2'),
        url('./font/MDGROTESQUE-Thin.woff') format('woff'),
        url('./font/MDGROTESQUE-Thin.ttf') format('truetype');
    font-weight: 100;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'MDGROTESQUE';
    src: url('MDGROTESQUE-BoldItalic.eot');
    src: local('MDGROTESQUE Bold Italic'), local('MDGROTESQUE-BoldItalic'),
        url('./font/MDGROTESQUE-BoldItalic.eot?#iefix') format('embedded-opentype'),
        url('./font/MDGROTESQUE-BoldItalic.woff2') format('woff2'),
        url('./font/MDGROTESQUE-BoldItalic.woff') format('woff'),
        url('./font/MDGROTESQUE-BoldItalic.ttf') format('truetype');
    font-weight: bold;
    font-style: italic;
    font-display: swap;
}

@font-face {
    font-family: 'MDGROTESQUE';
    src: url('MDGROTESQUE-Regular.eot');
    src: local('MDGROTESQUE Regular'), local('MDGROTESQUE-Regular'),
        url('./font/MDGROTESQUE-Regular.eot?#iefix') format('embedded-opentype'),
        url('./font/MDGROTESQUE-Regular.woff2') format('woff2'),
        url('./font/MDGROTESQUE-Regular.woff') format('woff'),
        url('./font/MDGROTESQUE-Regular.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'MDGROTESQUE';
    src: url('MDGROTESQUE-Italic.eot');
    src: local('MDGROTESQUE Italic'), local('MDGROTESQUE-Italic'),
        url('./font/MDGROTESQUE-Italic.eot?#iefix') format('embedded-opentype'),
        url('./font/MDGROTESQUE-Italic.woff2') format('woff2'),
        url('./font/MDGROTESQUE-Italic.woff') format('woff'),
        url('./font/MDGROTESQUE-Italic.ttf') format('truetype');
    font-weight: normal;
    font-style: italic;
    font-display: swap;
}



	body{
		font-family: 'MDGROTESQUE';
        margin: 0%;
        padding: 0%;
        width: 100%;
        height: 100%;
		
	}
	font-family: 'MD Grotesk Regular';

	html {
		font-family: system-ui, sans-serif;
		background-color: #F6F6F6;
	}
	code {
		font-family: Menlo, Monaco, Lucida Console, Liberation Mono, DejaVu Sans Mono,
			Bitstream Vera Sans Mono, Courier New, monospace;
	}
</style>
